<tr>
  <td class="align-middle">
    <%= content_tag :span, test.title, class: 'test-title', data: { test_id: test.id } %>
    <%= render 'form_inline', test: test == @test ? @test : test %>
  </td>
  <td class="align-middle"><%= test_level(test) %></td>
  <td class="align-middle"><%= test.questions.count %></td>
  <td class="align-middle"><%= test.timer %></td>
  <td class="align-middle"><%= link_to 'Show', admin_test_path(test), class: 'btn' %></td>
  <td class="align-middle">
    <%= link_to 'Edit', edit_admin_test_path(test),
                  class: 'btn form-inline-link', data: { test_id: test.id } %>
  </td>
  <td class="align-middle">
    <%= button_to 'Destroy', admin_test_path(test), class: 'btn',
                  method: :delete, data: { confirm: 'Are you sure?' }%>
  </td>
</tr>
