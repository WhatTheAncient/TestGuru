<tr>
  <td style="text-align: center"><%= test.title %></td>
  <td style="text-align: center"><%= test_level(test) %></td>
  <td style="text-align: center"><%= test.questions.count %></td>
  <td><%= button_to 'Show', admin_test_path(test), method: :get %></td>
  <td><%= button_to 'Destroy', admin_test_path(test), method: :delete, data: { confirm: 'Are you sure?' }%></td>
</tr>
