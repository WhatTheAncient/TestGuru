<tr>
  <td class="align-middle">
    <%= test.title %>
    <%= render 'form_inline', test: test == @test ? @test : test %>
  </td>
  <td class="align-middle"><%= test_level(test) %></td>
  <td class="align-middle"><%= test.questions.count %></td>
  <td class="align-middle"><%= button_to 'Show', admin_test_path(test), method: :get %></td>
  <td class="align-middle"><%= button_to 'Destroy', admin_test_path(test), method: :delete, data: { confirm: 'Are you sure?' }%></td>
  <td class="align-middle"><%= button_to 'Edit', edit_admin_test_path(test), method: :get %></td>
</tr>
